<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:maskimage="http://schemas.android.com/apk/res/com.hotveryspicy.maskimage"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:background="@drawable/ic_launcher"
        android:layout_width="300dp"
        android:layout_height="400dp">

    <com.hotveryspicy.maskimage.MaskImage
        android:id="@+id/imageview_id"
        maskimage:image="@drawable/nature"
        maskimage:mask="@drawable/mask"
        maskimage:frame="@drawable/transparent"
        android:layout_width="200dp"
        android:layout_height="200dp"/>
    </LinearLayout>
</RelativeLayout>
